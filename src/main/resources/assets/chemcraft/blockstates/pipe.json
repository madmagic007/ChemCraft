{
  "multipart": [
    {
      "apply": {
        "model": "chemcraft:block/pipe_disconnected"
      }
    },
    {
      "when": {
        "north": "1"
      },
      "apply": {
        "model": "chemcraft:block/pipe_segment"
      }
    },
    {
      "when": {
        "south": "1"
      },
      "apply": {
        "model": "chemcraft:block/pipe_segment",
        "y": 180
      }
    },
    {
      "when": {
        "east": "1"
      },
      "apply": {
        "model": "chemcraft:block/pipe_segment",
        "y": 90
      }
    },
    {
      "when": {
        "west": "1"
      },
      "apply": {
        "model": "chemcraft:block/pipe_segment",
        "y": 270
      }
    },
    {
      "when": {
        "up": "1"
      },
      "apply": {
        "model": "chemcraft:block/pipe_segment",
        "x": 270
      }
    },
    {
      "when": {
        "down": "1"
      },
      "apply": {
        "model": "chemcraft:block/pipe_segment",
        "x": 90
      }
    }
  ]
}